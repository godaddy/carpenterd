{
  "name": "carpenterd",
  "version": "2.4.2",
  "description": "Build and compile npm packages",
  "main": "./lib/index.js",
  "bin": {
    "carpenterd": "bin/server"
  },
  "scripts": {
    "start": "bin/server",
    "report": "nyc report --reporter=lcov",
    "test": "nyc npm run test:mocha",
    "test:mocha": "mocha test/**/*.test.js",
    "watch": "mocha --watch --reporter spec $(find test -path test/fixtures -prune -o -name '*.test.js')",
    "eslint": "eslint-godaddy -c .eslintrc lib/ test/lib/",
    "pretest": "npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/godaddy/carpenterd.git"
  },
  "keywords": [
    "build",
    "compile",
    "carpenter",
    "webpack",
    "builder",
    "widget",
    "component"
  ],
  "author": "GoDaddy.com Operating Company LLC",
  "license": "MIT",
  "dependencies": {
    "async": "~1.4.2",
    "babel": "~5.8.20",
    "bffs": "^5.0.0",
    "bl": "~1.1.2",
    "body-parser": "^1.18.3",
    "browserify": "~12.0.1",
    "cdnup": "^2.2.1",
    "clean-css": "~3.4.1",
    "create-servers": "~2.3.0",
    "datastar": "^1.6.0",
    "diagnostics": "~1.0.1",
    "emits": "~3.0.0",
    "errs": "~0.3.2",
    "feedsme-api-client": "^2.0.0",
    "fingerprinting": "~1.0.0",
    "from2": "^2.3.0",
    "joi": "~6.7.1",
    "json-try-parse": "~1.0.0",
    "lodash.intersection": "~3.2.0",
    "lodash.omit": "^4.5.0",
    "minimist": "~1.2.0",
    "minimize": "~1.7.0",
    "mkdirp": "^0.5.1",
    "node-uuid": "~1.4.3",
    "npm": "^5.8.0",
    "nsq-stream": "^2.1.0",
    "nsq.js-k8": "^1.1.1",
    "object-assign": "~4.0.1",
    "one-time": "~0.0.4",
    "repair": "~0.1.0",
    "resolve": "~1.3.3",
    "retryme": "^1.0.0",
    "safe-regex": "~1.1.0",
    "slay": "~1.0.1",
    "slay-config": "^2.0.1",
    "slay-contextlog": "~2.0.0",
    "slay-log": "^2.1.0",
    "tar-fs": "~1.8.1",
    "through2": "~2.0.0",
    "toml": "^2.3.0",
    "uglify-js": "^2.7.5",
    "walk": "~2.3.9",
    "warehouse-models": "^5.0.0",
    "webpack": "^1.12.12",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "assume": "^1.5.2",
    "assume-sinon": "^1.0.0",
    "babel-eslint": "^6.1.2",
    "eslint": "^4.4.1",
    "eslint-config-godaddy": "^2.0.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^4.11.0",
    "hyperquest": "~1.2.0",
    "mocha": "^5.2.0",
    "nock": "2.10.0",
    "nyc": "^11.8.0",
    "rip-out": "^1.0.0",
    "sinon": "^5.0.10"
  }
