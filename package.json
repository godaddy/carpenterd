{
  "name": "carpenterd",
  "version": "1.4.0",
  "description": "Build and compile npm packages",
  "main": "./lib/index.js",
  "scripts": {
    "start": "bin/server",
    "test": "istanbul cover _mocha -- --reporter spec $(find test -path test/fixtures -prune -o -name '*.test.js')",
    "watch": "mocha --watch --reporter spec $(find test -path test/fixtures -prune -o -name '*.test.js')",
    "eslint": "godaddy-js-style-eslint -c .eslintrc lib/ test/lib/",
    "jscs": "godaddy-js-style-jscs lib/ test/lib/",
    "lint": "npm run jscs && npm run eslint",
    "pretest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/godaddy/carpenterd.git"
  },
  "keywords": [
    "build",
    "compile",
    "carpenter",
    "webpack",
    "builder",
    "widget",
    "component"
  ],
  "author": "GoDaddy.com Operating Company LLC",
  "license": "MIT",
  "dependencies": {
    "async": "~1.4.2",
    "babel": "~5.8.20",
    "bffs": "^4.0.0",
    "bl": "~1.1.2",
    "body-parser": "~1.13.2",
    "browserify": "~12.0.1",
    "clean-css": "~3.4.1",
    "create-servers": "~2.3.0",
    "datastar": "~1.3.1",
    "emits": "~3.0.0",
    "errs": "~0.3.2",
    "feedsme-api-client": "^1.2.0",
    "fingerprinting": "~1.0.0",
    "gjallarhorn": "~1.3.0",
    "ioredis": "~1.15.0",
    "joi": "~6.7.1",
    "json-try-parse": "~1.0.0",
    "lodash.intersection": "~3.2.0",
    "minimist": "~1.2.0",
    "minimize": "~1.7.0",
    "mkdirp": "^0.5.1",
    "node-uuid": "~1.4.3",
    "npm": "~3.10.0",
    "object-assign": "~4.0.1",
    "one-time": "~0.0.4",
    "warehouse-models": "^5.0.0",
    "repair": "~0.1.0",
    "safe-regex": "~1.1.0",
    "slay": "^1.0.0",
    "slay-config": "^2.0.1",
    "slay-contextlog": "^1.0.0",
    "slay-log": "^2.1.0",
    "tar-fs": "~1.8.1",
    "through2": "~2.0.0",
    "toml": "^2.3.0",
    "uglify-js": "git://github.com/Swaagie/UglifyJS2.git#fcb4f2f",
    "walk": "~2.3.9",
    "webpack": "^1.12.12",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "assume": "^1.4.1",
    "babel-eslint": "^6.1.2",
    "eslint-plugin-json": "~1.2.0",
    "eslint-plugin-mocha": "~2.0.0",
    "eslint-plugin-react": "~4.2.0",
    "godaddy-style": "^3.1.6",
    "hyperquest": "~1.2.0",
    "istanbul": "~0.4.5",
    "mocha": "~3.1.0",
    "nock": "2.10.0"
  }
}
